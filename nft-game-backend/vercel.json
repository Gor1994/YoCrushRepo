{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://yo-crush-repo.vercel.app"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET,HEAD,PUT,PATCH,POST,DELETE"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          }
        ]
      }
    ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/main"
    }
  ]
  }
  